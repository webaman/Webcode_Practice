     <section class="content" >
      <div class="container-fluid" >
        <!-- SELECT2 EXAMPLE -->
        <div class="row mt-4">
          <div class="mx-auto card rounded" style="width: 40rem;">
            <div class="card-header">
              <h3 class="card-title">User Registration</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <form class="form-horizontal" method="post" enctype="multipart/form-data">
              <div class="card-body">
                <div class="form-group row">
                  <label for="inputEmail3" class="col-sm-3 col-form-label">Username</label>
                  <div class="col-sm-6">
                    <input type="text" class="form-control" name="uname" id="inputEmail3" value="{{mydata.user_name}}" placeholder="Username">
                  </div>
                </div>
              
                <div class="form-group row mt-4">
                  <label for="inputEmail3" class="col-sm-3 col-form-label">Useremail</label>
                  <div class="col-sm-6">
                    <input type="email" class="form-control" name="eml" id="inputEmail3" value="{{mydata.user_email}}" placeholder="Email">
                  </div>
                </div>
                <div class="form-group row mt-4">
                  <label for="inputEmail3" class="col-sm-3 col-form-label">Password</label>
                  <div class="col-sm-6">
                    <input type="password" class="form-control" name="pass" id="inputEmail3"  value="{{mydata.user_pass}}" placeholder="Password">
                  </div>
                </div>
                <div class="form-group row mt-4">
                  <label for="inputEmail3" class="col-sm-3 col-form-label" >DOB</label>
                  <div class="col-sm-6">
                    <div class="input-group date" id="reservationdate" data-target-input="nearest">
                      <input type="text" name="dob" value="{{mydata.user_dob}}" class="form-control datetimepicker-input" data-target="#reservationdate" placeholder="Select DOB"/>
                      <div class="input-group-append" data-target="#reservationdate" data-toggle="datetimepicker">
                          <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                      </div>
                  </div>
                  </div>
                </div>
               
                <div class="form-group row mt-4">
                  <label for="inputEmail3" class="col-sm-3 col-form-label">Gender</label>
                  <div class="col-sm-6">
                <div class="form-check">
                  <input class="form-check-input" type="radio" name="user_gen" id="flexRadioDefault1" value="Male" value="{{mydata.user_gender}}" {{#compare mydata.user_gender "==" "Male"}}checked{{/compare}} >
                  <label class="form-check-label" for="flexRadioDefault1">
                    Male
                  </label>
                </div>
                <div class="form-check">
                  <input class="form-check-input" type="radio" name="user_gen" id="flexRadioDefault2" value="Female" value="{{mydata.user_gender}}" {{#compare mydata.user_gender "=="  (toString "Female") }}checked{{/compare}} >
                  <label class="form-check-label" for="flexRadioDefault2">
                    Female
                  </label>
                </div>
                </div>
                </div>
                <div class="form-group row mt-4">
                  <label for="inputEmail3" class="col-sm-3 col-form-label">City</label>
                  <div class="col-sm-6">
                   <select class="form-group" aria-label="Default select example" name="_citycat" style="width: 290px; height:40px" >
  
   {{#each mydata}}
                        
                            <option value='{{_id}}'>{{city_name}}</option>
                           
                           {{/each}}
  
</select>
                </div>
                </div>
                <div class="form-group row mt-4">
